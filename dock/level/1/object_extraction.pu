@startuml Memo app

!define TRANSACTION_MARK_COLOR FFAA00

package "Memo app" as memo_app {

  entity "メモ" as memo <<M,MASTER_MARK_COLOR>> {
      タイトル
      --
      確認
      新規作成
  }

  entity "あるメモ" as memo_detail <<M,MASTER_MARK_COLOR>> {
      内容
      タイトル
      --
      確認
      変更
      削除
  }
}

memo - memo_detail

@enduml
